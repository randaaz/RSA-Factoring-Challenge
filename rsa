#!/usr/bin/python3
import math
import sys

def factorize(n):
    factors = []
    divisor = 2
    
    while n > 1:
        while n % divisor == 0:
            factors.append(divisor)
            n //= divisor
        divisor += 1
    
    return factors

def main(filename):
    with open(filename, 'r') as file:
        for line in file:
            n = int(line.strip())
            factors = factorize(n)
            # Sort the factors in descending order
            factors.sort(reverse=True)
            print(f"{n} =", "*".join(map(str, factors)))

if __name__ == "__main__":
    if len(sys.argv) != 2:
        print("Usage: python3 script.py input_filename")
        sys.exit(1)
    
    input_filename = sys.argv[1]
    main(input_filename)

